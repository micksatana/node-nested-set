{
  "defaultSeverity": "error",
  "extends": "tslint:recommended",
  "jsRules": {},
  "rules": {
    "max-line-length": false,
    "no-console": false,
    "no-empty": false,
    "no-unused-expression": false,
    "one-variable-per-declaration": false,
    "prefer-const": false,
    "quotemark": [true, "single"],
    "trailing-comma": false
  },
  "rulesDirectory": []
}
